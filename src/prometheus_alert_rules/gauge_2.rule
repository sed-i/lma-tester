- alert: Gauge_2_Trigger
  expr: gauge_2{%%juju_topology%%} > 10
  for: 0m
  labels:
    severity: High
  annotations:
    summary: "Instance {{ $labels.instance }} has gauge_2 higher than 10"
    description: "{{ $labels.instance }} of job {{ $labels.job }} has the value of gauge_2 higher than 10"
