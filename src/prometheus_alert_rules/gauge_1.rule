- alert: Gauge_1_Trigger
  expr: gauge_1{%%juju_topology%%} > 10
  for: 0m
  labels:
    severity: High
  annotations:
    summary: "Instance {{ $labels.instance }} has gauge_1 higher than 10"
    description: "{{ $labels.instance }} of job {{ $labels.job }} has the value of gauge_1 higher than 10"
